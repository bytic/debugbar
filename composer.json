{
  "name": "bytic/debugbar",
  "type": "library",
  "description": "Php Framework",
  "homepage": "https://github.com/bytic/debugbar",
  "license": "MIT",
  "authors": [
    {
      "name": "Gabriel Solomon",
      "email": "hello@gabrielsolomon.ro"
    }
  ],
  "autoload": {
    "psr-4": {
      "Nip\\DebugBar\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Nip\\DebugBar\\Tests\\": "tests/src"
    }
  },
  "require": {
    "bytic/debug": "^0.9|^1.0|^2.0",
    "bytic/logger": "^0.9|^1.0|^2.0",
    "bytic/http": "^0.9|^1.0|^2.0",
    "maximebf/debugbar": "^1.17"
  },
  "require-dev": {
    "bytic/container": "^0.9|^1.0|^2.0",
    "bytic/database": "^0.9|^1.0|^2.0",
    "bytic/router": "^0.9|^1.0|^2.0",
    "bytic/phpqatools": "^1.0"
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "phpstan/extension-installer": true,
      "ergebnis/composer-normalize": true,
      "bytic/phpqatools": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
